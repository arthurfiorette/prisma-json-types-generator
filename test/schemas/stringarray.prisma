generator client {
  provider = "prisma-client"
  output   = "../target/stringarray"
}

generator json {
  provider  = "node ./index.js"
  namespace = "PStringArrayJson"
}

datasource db {
  provider = "postgresql"
}

model FakeModel {
  id String   @id @default(uuid())  @db.Uuid
  /// [MyFakeStringType]
  fakeTags        String[] @default([])
}
