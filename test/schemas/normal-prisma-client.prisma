generator client {
  provider = "prisma-client"
  output   = "../target/normal-prisma-client"
  importFileExtension = "js"
}

generator json {
  provider = "node ./index.js"
  namespace = "PNormalPrismaClientJson"
  allowAny = true
}

datasource db {
  provider = "postgresql"
  url      = ""
}

model Model {
  id Int @id @default(autoincrement())

  /// [Simple]
  simple Json

  /// [Optional]
  optional Json?

  /// [List]
  list Json[]
}

model pages {
  id         String   @id
  /// [pages.pay_provider] 
  pay_provider    String 
  /// [pages.ats_config] 
  ats_config          Json?
}
