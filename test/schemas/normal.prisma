generator client {
  provider = "prisma-client"
  output   = "../target/normal"
}

generator json {
  provider = "node ./index.js"
  namespace = "PNormalJson"
  allowAny = true
}

datasource db {
  provider = "postgresql"
}

model Model {
  id Int @id @default(autoincrement())

  /// [Simple]
  simple Json

  /// [Optional]
  optional Json?

  /// [List]
  list Json[]
}

// Test case matching the reproduction repository
// https://github.com/rivatove/prisma-json-types-generator-418
model orders {
  id Int @id @default(autoincrement())
  
  /// [SampleJson]
  meta Json?
}
